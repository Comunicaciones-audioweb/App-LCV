<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="mx.com.audioweb.lcv.MainScreen"
    android:background="@drawable/login_bg2">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:layout_marginTop="53dp"
        android:src="@drawable/logo_audioweb"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageBUr"
        android:src="@drawable/unirse_reunion"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:background="@android:color/transparent"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageBVr"
        android:src="@drawable/visualizar_reunion"
        android:background="@android:color/transparent"
        android:layout_below="@+id/imageBUr"
        android:layout_alignRight="@+id/imageBUr"
        android:layout_alignEnd="@+id/imageBUr"
        android:layout_alignStart="@+id/imageBUr"
        android:layout_marginTop="10dp" />

    <ImageButton
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageBAc"
        android:layout_below="@+id/imageBVr"
        android:src="@drawable/atencion_clientes2"
        android:background="@android:color/transparent"
        android:layout_alignLeft="@+id/imageBVr"
        android:layout_alignStart="@+id/imageBVr" />
</RelativeLayout>
